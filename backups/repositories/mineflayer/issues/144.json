            "created_at": "2015-03-17T10:02:09Z",
            "html_url": "https://github.com/andrewrk/mineflayer/issues/144#issuecomment-82254201",
            "id": 82254201,
            "issue_url": "https://api.github.com/repos/andrewrk/mineflayer/issues/144",
            "updated_at": "2015-03-17T10:02:09Z",
            "url": "https://api.github.com/repos/andrewrk/mineflayer/issues/comments/82254201",
            "user": {
                "avatar_url": "https://avatars.githubusercontent.com/u/1069318?v=3",
                "events_url": "https://api.github.com/users/roblabla/events{/privacy}",
                "followers_url": "https://api.github.com/users/roblabla/followers",
