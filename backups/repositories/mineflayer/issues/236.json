                "login": "Darthfett",
                "organizations_url": "https://api.github.com/users/Darthfett/orgs",
                "received_events_url": "https://api.github.com/users/Darthfett/received_events",
                "repos_url": "https://api.github.com/users/Darthfett/repos",
                "site_admin": false,
